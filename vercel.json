{
  "rewrites": [
    {
      "source": "/auth/instagram/callback",
      "destination": "/index.html"
    },
    {
      "source": "/auth/instagram/callback(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/auth/instagram/callback?code=(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/auth/instagram/callback?state=(.*)&code=(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 