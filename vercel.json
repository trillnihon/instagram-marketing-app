{
  "rewrites": [
    {
      "source": "/auth/instagram/callback",
      "destination": "/index.html"
    },
    {
      "source": "/auth/instagram/callback(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/auth/instagram",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/index.html"
    },
    {
      "source": "/register",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/auth/instagram/callback",
      "destination": "/index.html",
      "permanent": false
    },
    {
      "source": "/auth/instagram/callback(.*)",
      "destination": "/index.html",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/auth/instagram/callback(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
} 