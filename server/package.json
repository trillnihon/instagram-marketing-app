{
  "name": "instagram-marketing-server",
  "version": "1.0.0",
  "type": "module",
  "main": "server.js",
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "preinstall": "node --version",
    "start": "node server.js",
    "be:start:debug": "node --trace-uncaught --trace-warnings server/server.js",
    "dev": "node --watch server.js",
    "dev:all": "concurrently \"npm run dev\" \"cd ../src && npm run dev\"",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "healthcheck": "tsx ../scripts/healthcheck.ts",
    "token:refresh": "tsx ../scripts/refresh-long-lived-token.ts",
    "token:rotate-now": "tsx ../scripts/refresh-long-lived-token.ts --rotate-now",
    "verify:graph": "tsx ../scripts/verify-graph.ts",
    "smoke:auth": "tsx ../scripts/smoke-auth.ts",
    "smoke:post": "tsx ../scripts/smoke-post.ts",
    "smoke:insights": "tsx ../scripts/smoke-insights.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@babel/generator": "^7.28.0",
    "@babel/parser": "^7.28.0",
    "@babel/template": "^7.27.2",
    "@babel/traverse": "^7.28.0",
    "@babel/types": "^7.28.1",
    "@google/generative-ai": "^0.24.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.5.0",
    "bcryptjs": "^3.0.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "csstype": "^3.1.3",
    "detect-port": "^2.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.1",
    "form-data": "^4.0.3",
    "helmet": "^8.1.0",
    "jest": "^30.0.5",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "mongoose": "^8.16.4",
    "openai": "^4.104.0",
    "stripe": "^14.25.0",
    "supertest": "^7.1.4",
    "undici": "^7.11.0",
    "undici-types": "^7.11.0",
    "uuid": "^9.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.13",
    "mongodb-memory-server": "^10.1.4",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
