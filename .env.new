# Instagram API設定
VITE_INSTAGRAM_APP_ID=your_instagram_app_id_here
VITE_INSTAGRAM_APP_SECRET=your_instagram_app_secret_here
VITE_INSTAGRAM_REDIRECT_URI=http://localhost:4000/auth/callback

# Facebook API設定
VITE_FACEBOOK_APP_ID=your_facebook_app_id_here
VITE_FACEBOOK_APP_SECRET=your_facebook_app_secret_here

# 開発環境設定
VITE_API_BASE_URL=http://localhost:4000
VITE_ENVIRONMENT=development

# OpenAI API設定（サーバーサイドで使用）
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4

# 使用制限設定
VITE_FREE_PLAN_CAPTION_LIMIT=10
VITE_PREMIUM_PLAN_CAPTION_LIMIT=100
VITE_ENTERPRISE_PLAN_CAPTION_LIMIT=1000

# サーバー設定
PORT=4000
REDIRECT_URI=http://localhost:4000/auth/callback 